{
  "name": "@ericrovell/rational",
  "version": "0.5.2",
  "description": "Rational is rational numbers library written in JavaScript",
  "main": "index.js",
  "module": "index.mjs",
  "scripts": {
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "check-types": "tsc --noEmit true",
    "test": "jest --config ./jest.config.json --coverage",
    "build": "rm -rf ./dist/* && rollup --config",
    "build:post": "cp *.json dist && cp *.md dist",
    "release": "cd dist && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericrovell/rational.git"
  },
  "keywords": [
    "math",
    "numbers",
    "rational",
    "fraction",
    "parser"
  ],
  "author": "Eric Rovell",
  "types": "index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.3",
    "rollup": "^2.58.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "simple-git-hooks": "^2.6.1",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "default": "./index.mjs"
    }
  },
  "homepage": "https://github.com/ericrovell/rational#readme",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">=12.0"
  }
}
